{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "pac",
      "camelcase": "Pac",
      "title": "Pixel Art Console",
      "scope": "source.pac",
      "file-types": [
        "pac"
      ],
      "injection-regex": "^pac$",
      "class-name": "TreeSitterPac"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A parser for the Pixel Art Console language",
    "authors": [
      {
        "name": "mckahz",
        "email": "fletchusmaximus@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-pac"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}